{
  "file_processing": {
    "file_path": "../../../dataset/cleaned/company_cleaned.csv",
    "file_extension_pattern": "*.csv",
    "filename_pattern": "company_cleaned",
    "encoding": "utf-8",
    "separator": ",",
    "read_mode": "bulk-pd"
  },
  "data_source": {
    "name": "wiki",
    "table_name": "esg_wiki",
    "create_table": "True",
    "if_table_exists": "drop",
    "primary_keys": ["wikidata_id"],
    "attributes": {
      "wikidata_id": ["company_id", "str"],
      "name": ["company_name", "str"],
      "country_name_short_name": ["country_name_short_name", "str"],
      "country_name_alpha2": ["country_name_alpha2", "str"],
      "hq_short_name": ["hq_short_name", "str"],
      "hq_alpha2": ["hq_alpha2", "str"],
      "lei_cleaned_id": ["lei", "str"],
      "perm_id": ["perm_id", "str"],
      "bloomberg_id": ["bloomberg_id", "str"],
      "isin_cleaned_id": ["isin", "str"],
      "siren_cleaned_id": ["siren", "str"],
      "siret_cleaned_id": ["siret", "str"],
      "name_clean": ["company_name_clean", "str"],
      "alias_clean": ["alias_clean", "str"]
    },
    "matching_role": "referential",
    "matching_alias": {
      "COMPANY": "company_name_clean",
      "COMPANY_OTHER_NAME1": "alias_clean",
      "COMPANY_OTHER_NAME2": "company_name_clean",
      "COMPANY_HQ_OTHER_NAME1": "alias_clean",
      "COMPANY_HQ_OTHER_NAME2": "company_name_clean",
      "COUNTRY": "country_name_alpha2",
      "COUNTRY_HQ": "country_name_alpha2",
      "LEI": "lei",
      "PERM_ID": "perm_id",
      "BLOOMBERG_ID": "bloomberg_id",
      "ISIN": "isin",
      "SIREN": "siren",
      "SIRET": "siret"
    },
    "map_to_matching": {
      "ref_id": "company_id",
      "ref_company": "company_name_clean",
      "ref_original_company": "company_name",
      "ref_country": "country_name_alpha2"
    }
  }
}
