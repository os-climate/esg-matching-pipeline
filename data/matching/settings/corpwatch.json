{
  "file_processing": {
    "file_path": "../../../dataset/cleaned/corpwatch_cleaned.csv",
    "file_extension_pattern": "*.csv",
    "filename_pattern": "corpwatch_cleaned",
    "encoding": "utf-8",
    "separator": ",",
    "read_mode": "bulk-pd"
  },

  "data_source": {
    "name": "corpwatch",
    "table_name": "esg_corpwatch",
    "create_table": "True",
    "if_table_exists": "drop",
    "primary_keys": ["company_id"],
    "attributes": {
      "company_id": ["company_id", "int"],
      "cw_id": ["cw_id", "int"],
      "cik": ["cik", "int"],
      "company_name": ["company_name", "str"],
      "company_name_lei": ["company_name_lei", "str"],
      "city": ["city", "str"],
      "country_code_short_name": ["country_code_short_name", "str"],
      "country_code_alpha2": ["country_code_alpha2", "str"],
      "lei_cleaned_id": ["lei", "str"],
      "company_name_clean": ["company_name_clean", "str"],
      "company_name_lei_clean": ["company_name_lei_clean", "str"]
    },

    "matching_role": "referential",
    "policy_name": "matching_with_corpwatch",
    "matching_alias": {
      "LEI": "lei",
      "PARENT_LEI": "lei",
      "COMPANY": "company_name_clean",
      "COUNTRY": "country_code_alpha2"
    },

    "map_to_matching": {
      "ref_id": "company_id",
      "ref_company": "company_name_clean",
      "ref_original_company": "company_name",
      "ref_country": "country_code_alpha2"
    }
  }
}
